{
  "version": 2,
  "name": "bijbelquiz-admin-portal",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/tracking",
      "dest": "/api/tracking.js"
    },
    {
      "src": "/api/errors",
      "dest": "/api/errors.js"
    },
    {
      "src": "/api/errors/(.*)",
      "dest": "/api/errors.js"
    },
    {
      "src": "/api/store",
      "dest": "/api/store.js"
    },
    {
      "src": "/api/store/(.*)",
      "dest": "/api/store.js"
    },
    {
      "src": "/api/messages",
      "dest": "/api/messages.js"
    },
    {
      "src": "/api/messages/(.*)",
      "dest": "/api/messages.js"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}